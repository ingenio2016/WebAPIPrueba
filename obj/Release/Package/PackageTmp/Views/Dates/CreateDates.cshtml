@model WebAPIPrueba.Models.Dates

@{
    ViewBag.Title = "Módulo de Reuniones";
    Layout = "~/Views/Shared/_Layout.cshtml";
    int id = Convert.ToInt32(ViewBag.Id);
    WebAPIPrueba.Models.WebApiPruebaContext db = new WebAPIPrueba.Models.WebApiPruebaContext();
    WebAPIPrueba.Models.TimesDates timesdate = db.TimesDates.Find(id);
    Model.EventDate = DateTime.Now;
}


<h2>Asignar nueva Reunión</h2>


@using (Html.BeginForm("Create", "Dates", FormMethod.Post))
{
    @Html.AntiForgeryToken()
    <div class="form-horizontal">
        @Html.ValidationSummary(true, "", new { @class = "text-danger" })
        @Html.HiddenFor(model => model.SystemDate)
        @Html.HiddenFor(model => model.ProfessionalId)

        <div class="form-group">
            <div class="col-md-4">
                @Html.LabelFor(model => model.EventDate, "Fecha Reunión", htmlAttributes: new { @class = "control-label" })
                <div class='input-group date' id='datepicker' style="width:280px">
                    <input type="text" name="EventDate" id="EventDate" value="@ViewData["Fecha"]" class="form-control" onblur="Change()" />
                    <span class="input-group-addon">
                        <span class="glyphicon glyphicon-calendar"></span>
                    </span>
                </div>
                @Html.ValidationMessageFor(model => model.EventDate, "", new { @class = "text-danger" })
            </div>
            <div class="col-md-4">
                @Html.LabelFor(model => model.ProfessionalId, "Solicitada por", htmlAttributes: new { @class = "control-label" })
                <input type="text" class="form-control" name="ProfessionalId" value="@Model.ProfessionalId" />                
            </div>
            <div class="col-md-4">
                <label class="control-label">Teléfono</label>
                <input type="text" class="form-control" name="Phone" value="@Model.Phone" />
            </div>
            
        </div>
        <div class="form-group">
            <div class="col-md-4">
                @Html.LabelFor(model => model.Hour, "Hora de la Reunión", htmlAttributes: new { @class = "control-label" })
                @Html.DropDownList("Hour", ViewData["Hours"] as List<SelectListItem>, htmlAttributes: new { @class = "form-control" })
                @Html.ValidationMessageFor(model => model.HourId, "", new { @class = "text-danger" })
            </div>
            <div class="col-md-4">
                @Html.LabelFor(model => model.Observation, "Motivo de la Reunión", htmlAttributes: new { @class = "control-label" })
                @Html.EditorFor(model => model.Observation, new { htmlAttributes = new { @class = "form-control" } })
                @Html.ValidationMessageFor(model => model.Observation, "", new { @class = "text-danger" })
            </div>
            <div class="col-md-4">
                <label class="control-label">Lugar de la Reunión</label><br />
                @Html.EditorFor(model => model.Address, new { htmlAttributes = new { @class = "form-control", @placeholder = "Direccion exacta del Lugar" } })
            </div>
            <div class="col-md-4">
                <label class="control-label">Cantidad de Asistentes</label><br />
                @Html.EditorFor(model => model.PersonsNumber, new { htmlAttributes = new { @class = "form-control" } })
                @Html.ValidationMessageFor(model => model.PersonsNumber, "", new { @class = "text-danger" })<br />
            </div>
            <div class="col-md-4">
                <label class="control-label">Moderador</label><br />
                @Html.EditorFor(model => model.Moderator, new { htmlAttributes = new { @class = "form-control", @placeholder = "Este campo puede ser editado luego" } })
                @Html.ValidationMessageFor(model => model.Moderator, "", new { @class = "text-danger" })<br />
                
            </div>
            <div class="col-md-4">
                <br />
                <input type="submit" value="Crear Reunion" class="btn btn-primary btn-sm" style="border-radius:25px; background-color:#000099;" />
                @Html.ActionLink("Volver", "Create", new { }, new { @class = "btn btn-success btn-sm", @style = "background-color:#333334;border-radius:25px" })
            </div>
        </div>
        <hr />
        
        <div class="form-group">
            <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12">
                <div class="form-group">
                    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                        <label>06:00 AM</label>
                    </div>
