                    {
                        <div class="col-lg-8 col-md-8 col-sm-12 col-xs-12">
                            <input style="background-color:#000099;color:white" type="text" value="" class="form-control" readonly />
                        </div>
                    }
                    else
                    {
                        var date = db.Dates.Find(timesdate.seis00);
